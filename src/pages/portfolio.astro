---
import MainLayout from '../layouts/MainLayout.astro';
import '../styles/basic-grid.css';
import {Picture} from "@astrojs/image/components";
//images
import dd_1_thumb from '../media/dd_1_thumb.png';
import lepta_thumb from '../media/lepta_thumb.png';
import gp_1_thumb from '../media/gp_1_thumb.png';
import w_5 from '../media/w_5.gif';
import ex_1_thumb from '../media/ex_1_thumb.png';
import myRISD_1_thumb from '../media/myRISD_1_thumb.png';
import v_1_thumb from '../media/v_1_thumb.png';
import c_6 from '../media/c_6.jpeg';
import co_1 from '../media/co_1.png';
---

<MainLayout title="Portfolio - Dom">

	<body>
		<section class="basic-grid">
			<a href="../dom/projects/daydream/">
